{% extends "layouts/default.html.twig" %}

{% block content %}
    <section id="main-content">
        <div class="container-fluid">
            {% set index = 0 %}

            {% for i in 0..4 %}
                <div class="row">
                    {% for j in 0..3 %}
                    <div class="col-xs-3 no-padding">
                        <img src="{{ result[index].images.low_resolution.url }}" class="img-responsive" />
                        {% set index = index + 1 %}
                    </div>
                    {% endfor %}
                </div>
            {% endfor %}
        </div>
    </section>

{% endblock content %}



